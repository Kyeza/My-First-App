<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kyezaarnold.myfirstapp">
    <uses-sdk android:minSdkVersion="11" android:maxSdkVersion="23" android:targetSdkVersion="23"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".MyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.kyezaarnold.myfirstapp.MyActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kyezaarnold.myfirstapp.MyActivity"/>
        </activity>
    </application>

</manifest>